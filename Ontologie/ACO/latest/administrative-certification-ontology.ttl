<https://w3id.org/italia/onto/ACO/> rdf:type owl:Ontology ;
                                     owl:versionIRI <https://w3id.org/italia/onto/ACO/1.0> ;
                                     owl:imports <https://w3id.org/italia/onto/CLV> ,
                                                 <https://w3id.org/italia/onto/l0> ;
                                     dct:accrualPeriodicity <http://publications.europa.eu/resource/authority/frequency/IRREG> ;
                                     dct:creator <https://w3id.org/italia/data/public-organization/ISTAT> ;
                                     dct:description "Questa è l'ontologia dei certificati anagrafici"@it ,
                                                     "This is the ontology of the administrative certifications."@en ;
                                     dct:identifier <https://w3id.org/italia/onto/ACO> ;
                                     dct:issued "2022-12-15"^^xsd:date ;
                                     dct:language <http://publications.europa.eu/resource/authority/language/ITA> ;
                                     dct:modified "2022-12-15"^^xsd:date ;
                                     dct:publisher "---" ;
                                     dct:rightsHolder <https://www.interno.gov.it/it> ;
                                     dct:title "ACO_IT - Administrative Certification Ontology"@en ,
                                               "ACO_IT - Ontologia dei Certificati Anagrafici"@it ;
                                     rdfs:comment "Questa è l'ontologia dei certificati anagrafici."@it ,
                                                  "This is the administrative certification ontology."@en ;
                                     rdfs:isDefinedBy <https://w3id.org/italia/onto/ACO> ;
                                     rdfs:label "Admministrative Certification Ontology"@en ,
                                                "Ontologia dei Certificati Anagrafici"@it ;
                                     owl:versionInfo "Versione 1.0"@it ;
                                     dcat:contactPoint " ---" ;
                                     dcat:keyword "Anagrafe"@it ,
                                                  "Certificato"@it ,
                                                  "Residente"@it ;
                                     dcat:theme <http://publications.europa.eu/resource/authority/data-theme/SOCI> ;
                                     admsapit:acronym "ACO" ;
                                     admsapit:hasContributor " ---" ;
                                     admsapit:hasFormalityLevel " ---" ;
                                     admsapit:hasKeyClass aco:Certificato ,
                                                          aco:Residenza ,
                                                          pro:PersonaAnagraficamenteResidente ;
                                     admsapit:hasOntologyLanguage admsapit:OWL ;
                                     admsapit:hasSemanticAssetDistribution <https://w3id.org/italia/data/distribution/CPVAPIT-RDF-TURTLE> ;
                                     admsapit:hasTask " ---" ;
                                     admsapit:last <https://w3id.org/italia/onto/ACO/https//w3id.org/italia/onto/ACO/1.0> ;
                                     admsapit:officialURI <https://w3id.org/italia/onto/ACO> ;
                                     admsapit:prefix "aco"^^xsd:string ;
                                     admsapit:status "published" ;
                                     admsapit:target "Pubbliche Amministrazioni" ;
                                     admsapit:type admsapit:DomainOntology .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#dateTime
xsd:dateTime rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#integer
xsd:integer rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/italia/onto/ACO/attesta
aco:attesta rdf:type owl:ObjectProperty ;
            rdfs:domain aco:AttoAmministrativo ;
            rdfs:comment "Relazione tra atto amministrativo e iscrizione anagrafica per nascita."@it ;
            rdfs:label "attesta"@it .


###  https://w3id.org/italia/onto/ACO/attestaNascita
aco:attestaNascita rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf aco:certifica ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain aco:CertificatoAnagraficoDiNascita ;
                   rdfs:range aco:IscrizioneAnagraficaPerNascita ;
                   rdfs:comment "Relazione tra l’atto amministrativo e iscrizione anagrafica per l’evento nascita."@it ;
                   rdfs:label "attesta nascita"@it .


###  https://w3id.org/italia/onto/ACO/attoEmessoDa
aco:attoEmessoDa rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain aco:AttoAmministrativo ;
                 rdfs:range aco:AmministrazionePubblica ;
                 rdfs:comment "Relazione tra atto amministrativo e amministrazione pubblica."@it ;
                 rdfs:label "atto emesso da"@it .


###  https://w3id.org/italia/onto/ACO/attoRegistratoIn
aco:attoRegistratoIn rdf:type owl:ObjectProperty ;
                     rdfs:domain aco:AttoAmministrativo ;
                     rdfs:range clv:AdminUnitComponent ;
                     rdfs:comment "Relazione tra atto amministrativo e unità amministrativa."@it ;
                     rdfs:label "atto registratoIn"@it .


###  https://w3id.org/italia/onto/ACO/certifica
aco:certifica rdf:type owl:ObjectProperty ;
              rdfs:domain aco:Certificato ;
              rdfs:comment "Relazione tra certificato ed evento registrato. "@it ;
              rdfs:label "certifica"@it .


###  https://w3id.org/italia/onto/ACO/certificaResidenza
aco:certificaResidenza rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf aco:certifica ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain aco:CertificatoAnagraficoDiResidenza ;
                       rdfs:range cpv:Residenza ;
                       rdfs:comment "Relazione tra certificato e residenza."@it ;
                       rdfs:label "certifica residenza"@it .


###  https://w3id.org/italia/onto/ACO/certificatoEmessoDa
aco:certificatoEmessoDa rdf:type owl:ObjectProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain aco:Certificato ;
                        rdfs:range aco:AmministrazionePubblica ;
                        rdfs:comment "Relazione tra certificato e amministrazione pubblica."@it ;
                        rdfs:label "certificato emesso da"@it .


###  https://w3id.org/italia/onto/ACO/certificatoSuRichiesta
aco:certificatoSuRichiesta rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ,
                                    owl:InverseFunctionalProperty ;
                           rdfs:domain aco:Certificato ;
                           rdfs:range aco:RichiestaDiCertificato ;
                           rdfs:comment "Relazione tra certificato e richiesta di certificato."@it ;
                           rdfs:label "certificato per richiesta"@it .


###  https://w3id.org/italia/onto/ACO/citaAtto
aco:citaAtto rdf:type owl:ObjectProperty ;
             rdfs:domain aco:Certificato ;
             rdfs:range aco:AttoAmministrativo ;
             rdfs:comment "Relazione tra certificato e atto amministrativo."@it ;
             rdfs:label "cita atto"@it .


###  https://w3id.org/italia/onto/ACO/depositatoPresso
aco:depositatoPresso rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain aco:AttoAmministrativo ;
                     rdfs:range aco:AmministrazionePubblica ;
                     rdfs:comment "Relazione tra atto amministrativo e amministrazione pubblica."@it ;
                     rdfs:label "depositato presso"@it .


###  https://w3id.org/italia/onto/ACO/faRiferimentoAdAttoDiNascita
aco:faRiferimentoAdAttoDiNascita rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf aco:attesta ;
                                 rdf:type owl:FunctionalProperty ;
                                 rdfs:domain aco:IscrizioneAnagraficaPerNascita ;
                                 rdfs:range aco:AttoDiNascita ;
                                 rdfs:comment "Relazione tra atto di nascita e iscrizione anagrafica per nascita."@it ;
                                 rdfs:label "fa riferimento ad atto di nascita"@it .


###  https://w3id.org/italia/onto/ACO/fornisceEvidenza
aco:fornisceEvidenza rdf:type owl:ObjectProperty ;
                     rdfs:domain aco:Certificato ;
                     rdfs:range aco:Evidenza ;
                     rdfs:label "fornisceEvidenza"@it .


###  https://w3id.org/italia/onto/ACO/haNascita
aco:haNascita rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ,
                       owl:InverseFunctionalProperty ;
              rdfs:domain cpv:PersonaFisica ;
              rdfs:range aco:IscrizioneAnagraficaPerNascita ;
              rdfs:comment "Relazione tra persona fisica e iscrizione anagrafica per nascita."@it ;
              rdfs:label "haNascita"@it .


###  https://w3id.org/italia/onto/ACO/haResidenza
aco:haResidenza rdf:type owl:ObjectProperty ,
                         owl:InverseFunctionalProperty ;
                rdfs:domain pro:PersonaAnagraficamenteResidente ;
                rdfs:range cpv:Residenza ;
                rdfs:comment "Relazione tra persona anagraficamente residente e residenza."@it ;
                rdfs:label "ha residenza"@it .


###  https://w3id.org/italia/onto/ACO/luogoIscrizioneAnagraficaperNascita
aco:luogoIscrizioneAnagraficaperNascita rdf:type owl:ObjectProperty ,
                                                 owl:FunctionalProperty ;
                                        rdfs:domain aco:IscrizioneAnagraficaPerNascita ;
                                        rdfs:range clv:AdminUnitComponent ;
                                        rdfs:comment "Dato che consente di stabilire, sotto il profilo spaziale, il Comune di iscrizione per nascita."@it ;
                                        rdfs:label "luogo di iscrizione anagrafica per nascita"@it .


###  https://w3id.org/italia/onto/ACO/residenzaInComune
aco:residenzaInComune rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain cpv:Residenza ;
                      rdfs:range clv:City ;
                      rdfs:comment "Relazione tra residenza e comune. "@it ;
                      rdfs:label "residenza in comune"@it .


###  https://w3id.org/italia/onto/ACO/residenzaInIndirizzo
aco:residenzaInIndirizzo rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain cpv:Residenza ;
                         rdfs:range clv:Address ;
                         rdfs:comment "Residenza stabilita in un indirizzo."@it ;
                         rdfs:label "residenza in indirizzo"@it .


###  https://w3id.org/italia/onto/ACO/ricopreRuolo
aco:ricopreRuolo rdf:type owl:ObjectProperty ,
                          owl:InverseFunctionalProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( aco:AmministrazionePubblica
                                             cpv:PersonaFisica
                                           )
                             ] ;
                 rdfs:range aco:RuoloRispettoARichiesta ;
                 rdfs:comment "Relazione tra persona fisica e autorità che rilascia il certificato o l’atto e il ruolo ricoperto, oppure interessato o delegato. "@it ;
                 rdfs:label "ricopre ruolo"@it .


###  https://w3id.org/italia/onto/ACO/rilasciatoConDirittiDiSegreteria
aco:rilasciatoConDirittiDiSegreteria rdf:type owl:ObjectProperty ,
                                              owl:FunctionalProperty ;
                                     rdfs:domain aco:Certificato ;
                                     rdfs:range aco:DirittiDiSegreteria ;
                                     rdfs:comment "Relazione tra certificato e diritti di segreteria."@it ;
                                     rdfs:label "rilasciato con diritti di segreteria"@it .


###  https://w3id.org/italia/onto/ACO/rilasciatoConImpostaDiBollo
aco:rilasciatoConImpostaDiBollo rdf:type owl:ObjectProperty ,
                                         owl:FunctionalProperty ;
                                rdfs:domain aco:Certificato ;
                                rdfs:range aco:ImpostaDiBollo ;
                                rdfs:comment "Relazione tra certificato e imposta di bollo."@it ;
                                rdfs:label "rilasciato con imposta di bollo"@it .


###  https://w3id.org/italia/onto/ACO/rispettoARichiesta
aco:rispettoARichiesta rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain aco:RuoloRispettoARichiesta ;
                       rdfs:range aco:RichiestaDiCertificato ;
                       rdfs:comment "Relazione tra ruolo rispetto a richiesta e richiesta di certificato."@it ;
                       rdfs:label "rispetto a richiesta"@it .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/italia/onto/ACO/annoAtto
aco:annoAtto rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain aco:AttoAmministrativo ;
             rdfs:range xsd:integer ;
             rdfs:comment "Anno di iscrizione nel registro riportante l'atto."@it ;
             rdfs:label "anno atto"@it .


###  https://w3id.org/italia/onto/ACO/dataDiEmissione
aco:dataDiEmissione rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain aco:Certificato ;
                    rdfs:range xsd:dateTime ;
                    rdfs:comment "Relazione tra certificato e data."@it ;
                    rdfs:label "data di emissione"@it .


###  https://w3id.org/italia/onto/ACO/dataDiIscrizioneAnagraficaPerNascita
aco:dataDiIscrizioneAnagraficaPerNascita rdf:type owl:DatatypeProperty ,
                                                  owl:FunctionalProperty ;
                                         rdfs:domain aco:IscrizioneAnagraficaPerNascita ;
                                         rdfs:range xsd:dateTime ;
                                         rdfs:comment "Data dell'iscrizione in anagrafe di una persona per nascita."@it ;
                                         rdfs:label "data di iscrizione anagrafica per nascita"@it .


###  https://w3id.org/italia/onto/ACO/dataDiScadenza
aco:dataDiScadenza rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain aco:Certificato ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "Durata del certificato: 6 mesi (art. 41 dpr 445/00), 3 mesi (art. 33 d. lgs. 223/1989). "@it ;
                   rdfs:label "data di scadenza"@it .


###  https://w3id.org/italia/onto/ACO/esito
aco:esito rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain aco:RichiestaDiCertificato ;
          rdfs:range xsd:string ;
          rdfs:comment "L'esito può essere di due tipi: accoglimento e rigetto della richiesta di certificato."@it ;
          rdfs:label "esito"@it .


###  https://w3id.org/italia/onto/ACO/motivazioneEsito
aco:motivazioneEsito rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain aco:RichiestaDiCertificato ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Motivazione dell'esito negativo della richiesta di certificato."@it ;
                     rdfs:label "motivazione esito"@it .


###  https://w3id.org/italia/onto/ACO/numeroAtto
aco:numeroAtto rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain aco:AttoAmministrativo ;
               rdfs:range xsd:string ;
               rdfs:comment "Identificativo dell'atto amministrativo."@it ;
               rdfs:label "numero atto"@it .


###  https://w3id.org/italia/onto/ACO/protocolloANPR
aco:protocolloANPR rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain aco:CertificatoAnagrafico ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Numero non progressivo (calcolato con algoritmo) unico e obbligatorio assegnato ad ogni certificato anagrafico rilasciato da ANPR e riportato all’interno del QRCode per il recupero della copia originale nel caso di richiesta di verifica."@it ;
                   rdfs:label "protocollo ANPR"@it .


###  https://w3id.org/italia/onto/ACO/tipoRuolo
aco:tipoRuolo rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain aco:RuoloRispettoARichiesta ;
              rdfs:range xsd:string ;
              rdfs:comment "il ruolo può essere di due tipi: richiedente e interessato alla richiesta di certificato."@it ;
              rdfs:label "tipo ruolo"@it .


###  https://w3id.org/italia/onto/ACO/tipologiaUtilizzo
aco:tipologiaUtilizzo rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain aco:Certificato ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Tipo di utilizzo del certificato."@it ;
                      rdfs:label "tipologia utilizzo"@it .


###  https://w3id.org/italia/onto/ACO/titoloAtto
aco:titoloAtto rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain aco:AttoAmministrativo ;
               rdfs:range xsd:string ;
               rdfs:comment "Intestazione dell'atto amministrativo."@it ;
               rdfs:label "titolo atto"@it .


###  https://w3id.org/italia/onto/CPV/codiceFiscale
cpv:codiceFiscale rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain cpv:PersonaFisica ;
                  rdfs:range xsd:string .


###  https://w3id.org/italia/onto/CPV/dateOfBirth
cpv:dateOfBirth rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain cpv:PersonaFisica ;
                rdfs:range xsd:dateTime .


###  https://w3id.org/italia/onto/CPV/familyName
cpv:familyName rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain cpv:PersonaFisica ;
               rdfs:range xsd:string .


###  https://w3id.org/italia/onto/CPV/givenName
cpv:givenName rdf:type owl:DatatypeProperty ;
              rdfs:domain cpv:PersonaFisica ;
              rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/italia/onto/ACO/AmministrazionePubblica
aco:AmministrazionePubblica rdf:type owl:Class ;
                            owl:equivalentClass cov:PublicOrganization ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty [ owl:inverseOf aco:attoEmessoDa
                                                                 ] ;
                                                  owl:someValuesFrom owl:Thing
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty [ owl:inverseOf aco:depositatoPresso
                                                                 ] ;
                                                  owl:someValuesFrom owl:Thing
                                                ] ;
                            rdfs:comment "Ente che svolge istituzionalmente una funzione amministrativa disciplinata da norme di diritto pubblico e da atti autoritativi e caratterizzata dalla formazione e dalla manifestazione della volontà della pubblica amministrazione o dal suo svolgersi per mezzo di poteri autoritativi o certificativi.  "@it ;
                            rdfs:label "Amministrazione pubblica"@it .


###  https://w3id.org/italia/onto/ACO/AttoAmministrativo
aco:AttoAmministrativo rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:attoEmessoDa ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:attoRegistratoIn ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:depositatoPresso ;
                                             owl:someValuesFrom owl:Thing
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:annoAtto ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:numeroAtto ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty aco:titoloAtto ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ;
                       rdfs:comment "Atto giuridico posto in essere da un'autorità amministrativa nell'esercizio di una sua funzione amministrativa."@it ;
                       rdfs:label "Atto amministrativo"@it .


###  https://w3id.org/italia/onto/ACO/AttoDelloStatoCivile
aco:AttoDelloStatoCivile rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Class ;
                                               owl:unionOf ( aco:AttoDiCittadinanza
                                                             aco:AttoDiDivorzio
                                                             aco:AttoDiMatrimonio
                                                             aco:AttoDiMorte
                                                             aco:AttoDiNascita
                                                             aco:AttoDiSeparazioneLegale
                                                             aco:AttoDiUnioneCivile
                                                           )
                                             ] ;
                         rdfs:subClassOf aco:AttoAmministrativo ;
                         rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sullo stato civile di una persona."@it ;
                         rdfs:label "Atto dello stato civile"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiCittadinanza
aco:AttoDiCittadinanza rdf:type owl:Class ;
                       rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sulla cittadinanza di una persona."@it ;
                       rdfs:label "Atto di cittadinanza"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiDivorzio
aco:AttoDiDivorzio rdf:type owl:Class ;
                   rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sul divorzio di una persona."@it ;
                   rdfs:label "Atto di divorzio"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiMatrimonio
aco:AttoDiMatrimonio rdf:type owl:Class ;
                     rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sul matrimonio di una persona."@it ;
                     rdfs:label "Atto di matrimonio"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiMorte
aco:AttoDiMorte rdf:type owl:Class ;
                rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sul decesso di una persona."@it ;
                rdfs:label "Atto di morte"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiNascita
aco:AttoDiNascita rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf aco:faRiferimentoAdAttoDiNascita
                                                       ] ;
                                        owl:someValuesFrom owl:Thing
                                      ] .


###  https://w3id.org/italia/onto/ACO/AttoDiSeparazioneLegale
aco:AttoDiSeparazioneLegale rdf:type owl:Class ;
                            rdfs:comment "Sentenza di separazione giudiziale o decreto di omologazione dell’accordo di separazione (artt. 709 bis e 711 c.p.c.) che l’ufficiale di stato civile annota sull’atto di matrimonio "@it ;
                            rdfs:label "Atto di separazione legale"@it .


###  https://w3id.org/italia/onto/ACO/AttoDiUnioneCivile
aco:AttoDiUnioneCivile rdf:type owl:Class ;
                       rdfs:comment "Atto rilasciato dall'ufficio di stato civile contenente le informazioni sull'unione civile di una persona."@it ;
                       rdfs:label "Atto di unione civile"@it .


###  https://w3id.org/italia/onto/ACO/Certificato
aco:Certificato rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty aco:certifica ;
