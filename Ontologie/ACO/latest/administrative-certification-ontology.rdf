        <rdfs:label xml:lang="it">Comune</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/codiceFiscale">
        <rdfs:label xml:lang="it">codiceFiscale</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/dateOfBirth">
