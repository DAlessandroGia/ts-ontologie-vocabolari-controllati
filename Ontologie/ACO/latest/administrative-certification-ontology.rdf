    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/italia/onto/ACO/Interessato"/>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/ACO/rispettoARichiesta"/>
                <owl:someValuesFrom>
